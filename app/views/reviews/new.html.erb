<h1>新規投稿</h1>
<!-- form_with部分 -->
<%= form_with model: @review, url: '/reviews', method: :post do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :review_title %>

  <h4>本文</h4>
  <%= f.text_area :review_body %>

  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>

  <%= f.submit '投稿' %>
<% end %>